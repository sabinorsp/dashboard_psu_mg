hospital_list <- function(esp){
data <- df_orig %>% filter(Especialidade == esp)
data$Hospital <- droplevels(data$Hospital)
table <- table(data$Hospital)
table_sort <- data.frame(table[order(table, decreasing=T)])
df_sum <- data %>% group_by(Hospital) %>%
summarise(media = mean(Total),
terc_quartil = quantile(Total)[4])
table_sort <- cbind(table_sort, data.frame(df_sum$media), data.frame(df_sum$terc_quartil))
colnames(table_sort) <- c('Hospitais', 'Qtd de inscritos', 'Média Nota Total', '3° Quartil')
return(table_sort)
}
